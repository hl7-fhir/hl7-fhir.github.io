  ];
  fhir:Practitioner.name [
     fhir:index 0;
     fhir:HumanName.family [
       fhir:value "Hippocrates";
       fhir:index 0
     ];
     fhir:HumanName.given [
       fhir:value "Harold";
       fhir:index 0
     ];
     fhir:HumanName.suffix [
       fhir:value "MD";
       fhir:index 0
     ]
  ] .

