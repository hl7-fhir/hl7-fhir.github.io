            },
            "TestScript.setup.action.operation.resource": {
              "code.value": "Patient"
            },
            "TestScript.setup.action.operation.description": {
              "string.value": "Attempt to read the Parameters resource type. What we really want here is an illegal type but the build process won't allow that. Parameters is a valid resource which doesn't have an end-point so, this should fail."
            },
            "TestScript.setup.action.operation.accept": {
              "code.value": "xml"
            },
            "TestScript.setup.action.operation.params": {
              "string.value": "/1"
            }
          }
        },
        {
          "TestScript.test.action.assert": {
            "TestScript.setup.action.assert.description": {
              "string.value": "Confirm that the returned HTTP status is 404(Not Found)."
            },
            "TestScript.setup.action.assert.response": {
              "code.value": "notFound"
            }
          }
        }
